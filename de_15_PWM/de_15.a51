ORG 0000H
	LJMP MAIN

ORG 00B0H
	LJMP PWM

OUT EQU P1.5
ORG 0030H
MAIN:
	MOV TMOD, #01H
	SETB ES
	SETB ET0
	
	MOV TL0, #LOW (-20000)
	MOV TH0, #HIGH (-20000)
	SETB TR0
	SETB OUT
	SJMP $
		
; NGAT TIMER0
PWM:
	CLR TR0
	
	CPL OUT
	
	MOV TL0, #LOW (-20000)
	MOV TH0, #HIGH (-20000)
	SETB TR0
RETI

END